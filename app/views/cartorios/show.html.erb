<%- model_class = Cartorio -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:codigo) %>:</strong></dt>
  <dd><%= @cartorio.codigo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @cartorio.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipoRegistro_id) %>:</strong></dt>
  <dd><%= @cartorio.atribuicao == nil ? 'N達o Informado' : @cartorio.atribuicao.tipoRegistro.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:atribuicao_id) %>:</strong></dt>
  <dd><%= @cartorio.atribuicao_id == nil ? 'N達o informado' : @cartorio.atribuicao.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comarca_id) %>:</strong></dt>
  <dd><%= @cartorio.comarca_id == nil ? 'N達o informado' : @cartorio.comarca.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cidade_id) %>:</strong></dt>
  <dd><%= @cartorio.cidade_id == nil ? 'N達o informado' : @cartorio.cidade.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bairro) %>:</strong></dt>
  <dd><%= @cartorio.bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rua) %>:</strong></dt>
  <dd><%= @cartorio.rua %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero) %>:</strong></dt>
  <dd><%= @cartorio.numero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:complemento) %>:</strong></dt>
  <dd><%= @cartorio.complemento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cep) %>:</strong></dt>
  <dd><%= @cartorio.cep %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>
  <dd><%= @cartorio.telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:celular) %>:</strong></dt>
  <dd><%= @cartorio.celular %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @cartorio.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:historico) %>:</strong></dt>
  <dd><%= @cartorio.historico %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              cartorios_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_cartorio_path(@cartorio), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              cartorio_path(@cartorio),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
