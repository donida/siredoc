<%- model_class = Atribuicao -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @atribuicao.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:chave) %>:</strong></dt>
  <dd><%= @atribuicao.chave %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipoRegistro_id) %>:</strong></dt>
  <dd><%= @atribuicao.tipoRegistro_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              atribuicaos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_atribuicao_path(@atribuicao), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              atribuicao_path(@atribuicao),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
